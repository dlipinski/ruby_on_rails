<h1>All Tags</h1>

<table class="table table-hover border">
    <tbody>
        <% @tags.each do |tag| %>
        <tr >
            <td class="media position-relative">
                <%= link_to tag.name, tag_path(tag), class: 'tag_name stretched-link', id: "tag_#{tag.id}" %>
            </td>
        </tr>
        <% end %>
    </tbody>
</table>